{
  "arrowParens": "always",
  "bracketSpacing": true,
  "bracketSameLine": false,
  "jsxSingleQuote": true,
  "printWidth": 120,
  "quoteProps": "as-needed",
  "semi": true,
  "singleQuote": true,
  "tabWidth": 2,
  "trailingComma": "all",
  "useTabs": false,
  "singleAttributePerLine": true,
  "importOrder": [
    "^@vercel/?(.*)?$",
    "^next/?(.*)?$",
    "^@carbon/?(.*)?$",
    "^antd/?(.*)?$",
    "^@ant-[a-zA-Z-_]{0,8}/?(.*)?$",
    "<THIRD_PARTY_MODULES>",
    "^[.]{2}[/]lib/(.*)$",
    "^[.]{2}[/]components/(.*)$",
    "^[./]"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
